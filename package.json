{
	"name": "visualizer",
	"module": "index.tsx",
	"type": "module",
	"scripts": {
		"dev": "vite --open /index.html",
		"prebuild": "bun run generate-pfp",
		"build": "vite build",
		"build:analyze": "vite build && npx vite-bundle-analyzer dist",
		"lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
		"preview": "vite preview",
		"test": "jest",
		"generate-pfp": "bun scripts/generate-pfp.ts"
	},
	"peerDependencies": {
		"typescript": "5.8.3"
	},
	"dependencies": {
		"@preact/signals": "1.3.0",
		"@tailwindcss/vite": "4.1.8",
		"preact": "10.26.5",
		"react-dom": "npm:@preact/compat",
		"react-dom/client": "npm:@preact/compat",
		"sharp": "0.34.4",
		"tailwindcss": "4.1.8"
	},
	"devDependencies": {
		"@preact/preset-vite": "2.10.1",
		"@types/bun": "latest",
		"@types/jest": "^29.5.12",
		"@types/three": "^0.169.0",
		"@typescript-eslint/eslint-plugin": "8.33.0",
		"@typescript-eslint/parser": "8.33.0",
		"eslint": "8.54.0",
		"eslint-config-prettier": "9.0.0",
		"eslint-plugin-import": "2.29.0",
		"eslint-plugin-prettier": "5.2.1",
		"eslint-plugin-react": "7.33.2",
		"eslint-plugin-react-hooks": "4.6.0",
		"jest": "^29.7.0",
		"prettier": "3.5.3",
		"rollup-plugin-visualizer": "6.0.1",
		"terser": "5.40.0",
		"ts-jest": "^29.2.3",
		"typescript": "5.8.3",
		"vite": "6.3.5",
		"vite-bundle-analyzer": "0.22.0",
		"vite-plugin-checker": "0.6.2"
	},
	"overrides": {
		"react": "npm:@preact/compat",
		"react-dom/test-utils": "npm:@preact/test-utils",
		"react-dom/client": "npm:@preact/compat",
		"react-dom": "npm:@preact/compat",
		"react/jsx-runtime": "npm:@preact/jsx-runtime"
	},
	"prettier": {
		"trailingComma": "es5",
		"semi": false,
		"singleQuote": true,
		"useTabs": true
